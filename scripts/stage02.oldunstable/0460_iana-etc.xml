<llpackages>
	<package 
		name="iana-etc"
		version="2.30"
		class="skel"
		install="destdir">
		<license>GPLv2</license>
		<sources>
			<file>
				<pkg sha1="218593bcb9264014c4e397d838b2c218eac9df06">iana-etc-2.30.tar.bz2</pkg>
				<mirror>http://sethwklein.net/</mirror>
			</file>
			<manualcheck date="20150823" interval="365" page="http://sethwklein.net/" />
			<check>
				<page html="http://sethwklein.net/iana-etc">
					<atext linktext="iana-etc-2.31." />
					<atext linktext="iana-etc-2.32." />
					<atext linktext="iana-etc-2.33." />
					<atext linktext="iana-etc-2.40." />
					<atext linktext="iana-etc-3." />
				</page>
			</check>
		</sources>
		<builddeps>
			 <dep>gcc</dep>
                        <dep>glibc</dep>
                        <dep>linux_headers</dep>
			<dep>file</dep>
			<dep>zlib</dep>
			<dep>bzip2</dep>
			<dep>pkg-config</dep>
			<dep>ncurses</dep>
			<dep>attr</dep>
			<dep>acl</dep>
			<dep>libcap</dep>
                        <dep>sed</dep>
			<dep>shadow</dep>
			<dep>psmisc</dep>
		</builddeps>
		<unpack>
			<![CDATA[ 
			tar xjf ${SRCDIR}/${PKGNAME}-${PKGVERSION}.tar.bz2
			]]>
		</unpack>
		<patch />
		<build>
			<![CDATA[
			cd ${PKGNAME}-${PKGVERSION}
			make
			]]>
		</build>
		<test />
		<install>
			<![CDATA[
			cd ${PKGNAME}-${PKGVERSION}
			make install DESTDIR=${TGTDIR}
			]]>
		</install>
		<clean>
			<![CDATA[
			rm -rf ${PKGNAME}-${PKGVERSION}
			]]>
		</clean>
	</package>
</llpackages>
